<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LinkUp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LinkUp"
    Shell.FlyoutBehavior="Disabled">
    
     <!--Do like this if you want to have multiple pages available.-->
    
    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        <ShellContent
            Title="My Teams"
            ContentTemplate="{DataTemplate local:TeamListPage}"
            Route="TeamListPage" />
        <ShellContent
            Title="GroupDeets"
            ContentTemplate="{DataTemplate local:GroupDetailsPage}"
            Route="GroupDetailsPage" />
        
        

    </TabBar>

    <!--<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->

</Shell>
